  </repository>
  <snapshotRepository>
     <id>snapshots</id>
     <name>Snapshots</name>
     <url>http://204.144.14.136:10000/nexus-2.3.0-04/content/repositories/snapshots/</url>
  </snapshotRepository>
</distributionManagement>


